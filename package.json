{
    "name": "live-captions",
    "version": "1.3.0",
    "main": "./build/index.js",
    "bin": "./build/index.js",
    "scripts": {
        "start": "npx tsc && node ./build/index.js",
        "start-gibberish": "npx tsc && node ./build/index.js --gibberish",
        "dist": "tsc && pkg ."
    },
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@colors/colors": "^1.6.0",
        "@google-cloud/speech": "^6.1.0",
        "audify": "^1.8.2",
        "bad-words": "^3.0.4",
        "body-parser": "^1.20.2",
        "express": "^4.18.2",
        "express-ws": "^5.0.2"
    },
    "devDependencies": {
        "@types/bad-words": "^3.0.3",
        "@types/express": "^4.17.21",
        "@types/express-ws": "^3.0.4",
        "@types/node": "^20.10.4",
        "pkg": "^5.8.1",
        "typescript": "^5.3.3"
    },
    "pkg": {
        "targets": [
            "node18-windows-x64"
        ],
        "assets": [
            "./src/public/**/*",
            "./node_modules/audify/**/*"
        ],
        "outputPath": "dist",
        "entry": "./build/index.js"
    }
}
